<%- include('partials/header') %>



<div class="container">
  <h1>Home</h1>
  <% posts.forEach(function(post){ %>
  <div class="well">
    <h3><%= post.title %></h3>
    <p><%= post.post.substring(0, 100) %>...</p>
    <a href="/posts/<%= post.title %>">Read more</a>
  </div>
  <% }); %>
</div>

<%- include('partials/footer') %>
